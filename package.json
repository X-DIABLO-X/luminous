{
  "name": "luminous",
  "icon": "icon.png",
  "displayName": "LUMINOUS",
  "description": "AI FOR ORGANIZING YOUR CODE",
  "version": "0.0.4",
  "publisher": "DIABLO",
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onView:luminous-sidebar"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "luminous-sidebar",
          "title": "Luminous",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "luminous-sidebar": [
        {
          "id": "luminous.view",
          "name": "Luminous Panel",
          "type": "webview",
          "contextualTitle": "Luminous"
        }
      ]
    },
    "commands": [
      {
        "command": "luminous.openSidebar",
        "title": "Open Luminous Sidebar"
      }
    ]
  },
  "main": "./extension.js",
  "categories": [
    "Other"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "compile": "tsc -p .",
    "package": "vsce package",
    "install": "npm install",
    "clean": "rimraf out"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "groq": "^3.72.1",
    "groq-sdk": "^0.13.0",
    "node-fetch": "^2.7.0"
  },"repository": {
  "type": "git",
  "url": "https://github.com/X-DIABLO-X/luminous.git"
}

}